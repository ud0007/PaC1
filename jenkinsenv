pipeline {
  agent any
  environment {
    subject = "Jenkins PaC"
    batchname = "Devopsbatch"
  }
  stages {
    stage ('my first stage') {
      steps {
        script {
          print "My subject is ${env.subject}"
          print "My batchname is ${env.batchname}"
        }
      }
    }
  }
}
